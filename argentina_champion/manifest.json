{

    "manifest_version": 2,
    "name": "Argentina Champion",
    "version": "1.0",
  
    "description": "Para continuar celebrando el triunfo de Argentina en la Copa del Mundo, cambia el 3 por tres estrellas y el 2 por Francia",
    "homepage_url": "https://github.com/Ivanknop/argentinaChampionWebExtension",
    "icons": {
      "48": "tres_estrellas.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "tres_estrellas.png",
      "default_title": "Argentina Champion"
    },
  
    "web_accessible_resources": [
      "tres_estrellas.jpg"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }

  